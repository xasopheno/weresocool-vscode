{
  "name": "socool",
  "publisher": "weresocool",
  "displayName": "weresocool",
  "description": "syntax highlighting for weresocool in vscode",
  "license": "MIT",
  "icon": "images/weresocool_icon.png",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/xasopheno/weresocool_vscode.git"
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "socool",
        "aliases": [
          "weresocool",
          "socool"
        ],
        "extensions": [
          "socool"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "socool",
        "scopeName": "source.weresocool",
        "path": "./syntaxes/socool.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.11.0"
  }
}
